<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Authors>Erik Johnson</Authors>
    <Company>Del Squared, LLC</Company>
    <RepositoryUrl>https://github.com/erjicles/Radicals</RepositoryUrl>
    <Copyright>Copyright © 2019 Erik Johnson</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageTags>radical radical-numbers radical-expressions biginteger bigrational c-sharp arbitrary-precision math arithmetic irrational square-roots nth-roots</PackageTags>
    <Description>.NET implementation of radical expressions enabling radical expression arithmetic.</Description>
    <PackageId>DelSquared.Radicals</PackageId>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>https://github.com/erjicles/Radicals/releases</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/erjicles/Radicals</PackageProjectUrl>
    <Version>1.0.0-beta3</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.3</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Open.Numeric.Primes" Version="1.5.4" />
    <PackageReference Include="Rationals" Version="1.3.3" />
  </ItemGroup>

</Project>
